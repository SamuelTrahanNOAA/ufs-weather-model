# This configuration is supposed to match the rrfs_dev1 parallel, so
# do not change the namelist without checking the current parallel.

export TEST_DESCR="HRRR physics on 13km domain, with radar-derived MP temperature tendencies for 1 hour"

export CNTL_DIR=rrfs_conus13km_radar_tten_warm

export LIST_FILES="sfcf000.nc \
                   sfcf001.nc \
                   sfcf002.nc \
                   atmf000.nc \
                   atmf001.nc \
                   atmf002.nc"

export_fv3
export_conus13km

export RESTART_INTERVAL=1
export FH_DFI_RADAR='0.0, 0.25, 0.50, 0.75, 1.0'

export THRD=2
export INPES=6
export JNPES=12
export TASKS=$(( INPES*JNPES*NTILES + WRTTASK_PER_GROUP*WRITE_GROUP ))
